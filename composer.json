{
    "name": "grom358/laravel-full-calendar",
    "description": "Laravel helper for FullCalendar.io. Fork of https://github.com/nelkasovic/laravel-full-calendar",
    "authors": [
        {
            "name": "LaravelFullCalendar by Cameron Zemek",
            "email": "grom358@gmail.com",
            "homepage": "https://cameronzemek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/helpers": "^1.0"
    },
    "license": "MIT",
    "homepage": "https://github.com/grom358/laravel-full-calendar",
    "support": {
        "email": "grom358@gmail.com",
        "issues": "https://github.com/grom358/laravel-full-calendar/issues",
        "wiki": "https://github.com/grom358/laravel-full-calendar/wiki",
        "source": "https://github.com/grom358/laravel-full-calendar",
        "docs": "https://github.com/grom358/laravel-full-calendar"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
